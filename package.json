{
  "name": "aave-ownership-data",
  "version": "0.1.0",
  "scripts": {
    "start": "node server.js",
    "build-contract": "solcjs contracts/AAVE.sol --abi -o abis --overwrite && solc contracts/AAVE.sol --bin -o bin --overwrite",
    "create": "graph create patrickcroke/aave-ownership-data --node https://api.thegraph.com/deploy/",
    "create-local": "graph create patrickcroke/aave-ownership-data --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy patrickcroke/aave-ownership-data --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy patrickcroke/aave-ownership-data --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "solc": "^0.8.4",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
